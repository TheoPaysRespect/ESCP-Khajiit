					<!--Art needed-->
					<li>UI/Deities/Theo_DeityKhenarthi</li>
					<!--Art needed-->
					<li>UI/Deities/ESCP_DeityMagnus</li>
				</iconPath>
				<overrideDeityGenders>true</overrideDeityGenders>
				<deityGenders>
					<li>Female</li>
					<li>None</li>
					<li>None</li>
					<li>Female</li>
					<li>Male</li>
				</deityGenders>
			</li>
