<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<!-- Link all graphics/files, set unique parameters-->
	<CultureDef>
		<defName>ESCP_KhajiitCulture</defName>
		<label>Khajiiti</label>
		<description>An ancient culture common amongst the Khajiiti kingdoms.</description>
		<pawnNameMaker>ESCP_Khajiit_NamerMale</pawnNameMaker>
		<pawnNameMakerFemale>ESCP_Khajiit_NamerFemale</pawnNameMakerFemale>
		<ideoNameMaker MayRequire="Ludeon.RimWorld.Ideology">NamerIdeoCorunan</ideoNameMaker>
		<deityNameMaker MayRequire="Ludeon.RimWorld.Ideology">ESCP_Khajiit_NamerCultureDeityName</deityNameMaker>
		<deityTypeMaker MayRequire="Ludeon.RimWorld.Ideology">DeityTypeMakerCorunan</deityTypeMaker>
		<festivalNameMaker MayRequire="Ludeon.RimWorld.Ideology">NamerFestivalCorunan</festivalNameMaker>
		<leaderTitleMaker MayRequire="Ludeon.RimWorld.Ideology">ESCP_Khajiit_NamerCultureLeaderTitle</leaderTitleMaker>
		<allowedPlaceTags>
			<li>OriginTribal</li>
		</allowedPlaceTags>
		<iconPath>World/WorldObjects/ESCP_Khajiit_Kingdom</iconPath>
		<thingStyleCategories>
			<li MayRequire="Ludeon.RimWorld.Ideology">
				<category>ESCP_KhajiitStyle</category>
				<priority>2</priority>
			</li>
			<li MayRequire="Ludeon.RimWorld.Ideology">
				<category>Animalist</category>
				<priority>1</priority>
			</li>
		</thingStyleCategories>
		<!--Piercing face tattoos, beards ok, prefer dreads-->
		<styleItemTags>
			<!--<li>
				<tag>ESCP_TattooFaceKhajiit</tag>
				<baseWeight>5</baseWeight>
				<weightFactor>1</weightFactor>
			</li>-->
			<li>
				<tag>Medieval</tag>
				<baseWeight>1</baseWeight>
				<weightFactor>1</weightFactor>
			</li>
			<li>
				<tag>Tribal</tag>
				<baseWeight>1</baseWeight>
				<weightFactor>1</weightFactor>
			</li>
			<li>
				<tag>NoBeard</tag>
				<baseWeight>20</baseWeight>
				<weightFactor>1</weightFactor>
			</li>
			<li>
				<tag>BeardTribal</tag>
				<baseWeight>0.1</baseWeight>
				<weightFactor>1</weightFactor>
			</li>
			<li>
				<tag>TattooFaceTribal</tag>
				<baseWeight>0.1</baseWeight>
				<weightFactor>1</weightFactor>
			</li>
			<li>
				<tag>TattooBodyTribal</tag>
				<baseWeight>0.1</baseWeight>
				<weightFactor>1</weightFactor>
			</li>
			<li>
				<tag>NoTattoo</tag>
				<baseWeight>1</baseWeight>
				<weightFactor>1</weightFactor>
			</li>
		</styleItemTags>
		<modExtensions>
			<li Class="ESCP_RaceTools.IdeoCultureProperties">
				<overrideDivines>true</overrideDivines>
				<!--Finish the divines-->
				<divinesList>
					<li>
						<name>Alkosh</name>
						<type>Dragon King of Cats</type>
						<gender>Male</gender>
						<iconPath>UI/Deities/ESCP_DeityAkatosh</iconPath>
					</li>
					<li>
						<name>Riddle'Thar</name>
						<type>The Two-Moons Dance</type>
						<gender>None</gender>
						<iconPath>Ideology/riddlethar</iconPath>
						<!--new-->
					</li>
					<li>
						<name>Jone</name>
						<type>the Little Moon</type>
						<gender>None</gender>
						<iconPath>Ideology/jone</iconPath>
						<!--new-->
					</li>
					<li>
						<name>Jode</name>
						<type>the Big Moon</type>
						<gender>None</gender>
						<iconPath>Ideology/jode</iconPath>
						<!--new-->
					</li>
					<li>
						<name>Khenarthi</name>
						<type>the Goddess of the Winds</type>
						<gender>Female</gender>
						<iconPath>Ideology/khenarthi</iconPath>
						<!--new-->
					</li>
					<li>
						<name>Rajhin</name>
						<type>the Footpad</type>
						<gender>Male</gender>
						<iconPath>Ideology/rajhin</iconPath>
					</li>
					<li>
						<name>S'rendarr</name>
						<type>the Runt</type>
						<gender>Male</gender>
						<iconPath>UI/Deities/ESCP_DeityStendarr</iconPath>
					</li>
					<li>
						<name>Baan Dar</name>
						<type>the Bandit God</type>
						<gender>Male</gender>
						<iconPath>UI/Deities/ESCP_DeityBaanDar</iconPath>
					</li>
				</divinesList>
			</li>
		</modExtensions>
	</CultureDef>
</Defs>