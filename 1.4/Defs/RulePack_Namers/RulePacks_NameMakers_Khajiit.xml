<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    <RulePackDef>
        <defName>Theo_Khajiit_NamerMale</defName>
        <include>
			<li>TribalWordUtility</li>
		</include>
		<rulePack>
			<rulesStrings>
				<li>name->[MaleList] [FamilyList]</li>
			</rulesStrings>
			<rulesFiles>
				<li>MaleList->Theo_KhajiitNames/KhajiitMale</li>
				<li>FamilyList->Theo_KhajiitNames/KhajiitFamily</li>
			</rulesFiles>
		</rulePack>
    </RulePackDef>

     <RulePackDef>
        <defName>Theo_Khajiit_NamerFemale</defName>
        <include>
			<li>TribalWordUtility</li>
		</include>
		<rulePack>
			<rulesStrings>
				<li>name->[FemaleList] [FamilyList]</li>
			</rulesStrings>
			<rulesFiles>
				<li>FemaleList->Theo_KhajiitNames/KhajiitFemale</li>
				<li>FamilyList->Theo_KhajiitNames/KhajiitFamily</li>
			</rulesFiles>
		</rulePack>
    </RulePackDef>
</Defs>